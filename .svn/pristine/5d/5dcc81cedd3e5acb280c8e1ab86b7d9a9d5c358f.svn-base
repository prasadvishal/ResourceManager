package com.mindfiresolutions.resourcemanager.model;

import com.google.gson.annotations.SerializedName;

/**
 * Created by Vishal Prasad on 5/10/2017.
 */

public class HardwareCount {
    @SerializedName("TotalHardware")
    private Integer totalHardware;
    @SerializedName("AvailableHardware")
    private Integer availableHardware;
    @SerializedName("AssignedHardware")
    private Integer assignedHardware;

    public Integer getTotalHardware() {
        return totalHardware;
    }

    public void setTotalHardware(Integer totalHardware) {
        this.totalHardware = totalHardware;
    }

    public Integer getAvailableHardware() {
        return availableHardware;
    }

    public void setAvailableHardware(Integer availableHardware) {
        this.availableHardware = availableHardware;
    }

    public Integer getAssignedHardware() {
        return assignedHardware;
    }

    public void setAssignedHardware(Integer assignedHardware) {
        this.assignedHardware = assignedHardware;
    }

}