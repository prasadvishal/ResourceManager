import com.google.gson.annotations.SerializedName;

import java.util.List;

public class HardwareSummaryCountWithResponse {

    @SerializedName("Response")
    private ResponseGetterBase response;
    @SerializedName("HardwareCountSummary")
    private List<ViewHardwareSummary> hardwareCountSummary = null;

    public ResponseGetterBase getResponse() {
        return response;
    }

    public void setResponse(ResponseGetterBase response) {
        this.response = response;
    }

    public List<ViewHardwareSummary> getHardwareCountSummary() {
        return hardwareCountSummary;
    }

    public void setHardwareCountSummary(List<ViewHardwareSummary> hardwareCountSummary) {
        this.hardwareCountSummary = hardwareCountSummary;
    }

}
